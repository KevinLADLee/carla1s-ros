<!-- -->
<launch>
  <arg name='host' default='localhost'/>
  <arg name='port' default='2000'/>
  <arg name='timeout' default='10'/>
  <arg name="role_name" default="ego_vehicle" />

  <node pkg="carla_global_planner" type="carla_global_planner_node.py" name="carla_global_planner_node" output="screen">
    <param name="role_name" value="$(arg role_name)" />
    <param name="host" value="$(arg host)" />
    <param name="port" value="$(arg port)" />
    <param name="timeout" value="$(arg timeout)" />
  </node>

  <node pkg="carla_global_planner" type="carla_map_visualization.py" name="carla_map_visualization" output="screen" />

</launch>
